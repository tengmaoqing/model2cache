{
  "name": "vue-data-cache",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "bar": "git add . && echo \"bar\" && exit 0",
    "rollup": "rollup -c",
    "lint-staged": "lint-staged"
  },
  "main": "dist/vueDataCache.umd.min.js",
  "dependencies": {},
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-latest": "^6.24.1",
    "lint-staged": "^8.1.0",
    "lodash": "^4.17.11",
    "pre-commit": "^1.2.2",
    "rollup": "^1.0.1",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "terser": "^3.14.1",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.5.17"
  },
  "pre-commit": "lint-staged",
  "gitHooks": {
    "commit-msg": "node scripts/verify-commit-msg.js"
  },
  "lint-staged": {
    "src/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "eslintIgnore": [
    "vueFormCache.common.js"
  ],
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
