{
  "name": "model2cache",
  "version": "1.0.0",
  "scripts": {
    "build": "node scripts/build.js",
    "commit": "git-cz",
    "release": "bash scripts/release.sh",
    "release:note": "node scripts/gen-release-note.js",
    "lint": "eslint src",
    "test:unit": "karma start test/karma.conf.js"
  },
  "main": "dist/model2cache.umd.js",
  "module": "dist/model2cache.esm.js",
  "types": "types/cache.d.ts",
  "dependencies": {
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@types/jasmine": "^3.5.10",
    "@types/lodash": "^4.14.149",
    "@types/lodash-es": "^4.17.3",
    "babel-eslint": "^10.0.1",
    "babel-loader": "^8.1.0",
    "conventional-changelog": "^3.0.5",
    "cz-conventional-changelog": "^2.1.0",
    "eslint": "^5.12.0",
    "eslint-plugin-vue": "^5.1.0",
    "husky": "^1.3.1",
    "karma": "^5.0.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-jasmine": "^3.1.1",
    "karma-rollup-preprocessor": "^7.0.3",
    "karma-typescript": "^5.0.2",
    "karma-typescript-es6-transform": "^5.0.2",
    "karma-webpack": "^4.0.2",
    "lint-staged": "^8.1.0",
    "lodash-es": "^4.17.15",
    "pre-commit": "^1.2.2",
    "rollup": "^1.0.1",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-typescript2": "^0.27.0",
    "terser": "^3.14.1",
    "typescript": "^3.8.3",
    "validate-commit-msg": "^2.14.0",
    "vue": "^2.6.11",
    "webpack": "^4.42.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "node scripts/verify-commit-msg.js"
    }
  },
  "lint-staged": {
    "src/*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tengmaoqing/vue-cache-data.git"
  },
  "keywords": [
    "vue",
    "cache",
    "form",
    "form2cache"
  ],
  "homepage": "https://github.com/tengmaoqing/vue-cache-data",
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
